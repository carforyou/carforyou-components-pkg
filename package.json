{
  "name": "@carforyou/components",
  "version": "0.0.0",
  "description": "CAR FOR YOU React components",
  "scripts": {
    "start": "http-server ./storybook-static -p 8008",
    "version": "npm run build",
    "dev": "npm run build:tailwind && npm run build:assets && storybook dev -p 6006",
    "test": "jest",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix",
    "build": "npm run build:tailwind && npm run build:assets && npm run build:pkg",
    "build:pkg": "rimraf pkg && rollup -c && tsc --outDir pkg/types --emitDeclarationOnly --declaration",
    "build:tailwind": "rimraf .tailwind && tsc --project tsconfig.tailwind.json",
    "build:assets": "npx cfy-assets build",
    "build:storybook": "npm run build:tailwind && storybook build",
    "bundlesize": "bundlesize",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carforyou/carforyou-components-pkg.git"
  },
  "author": "CAR FOR YOU",
  "license": "MIT",
  "homepage": "https://github.com/carforyou/carforyou-components-pkg#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "tailwindcss": "^2.2.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "downshift": "^7.0.0",
    "glob": "^7.1.6",
    "lodash.debounce": "^4.0.8",
    "react-calendar": "^4.0.0",
    "react-paginate": "^8.0.0",
    "react-range": "^1.8.11",
    "react-tooltip-lite": "npm:@carforyou/react-tooltip-lite@1.12.1",
    "style-loader": "3.3.3",
    "use-deep-compare-effect": "^1.4.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@carforyou/assets": "2.1.37",
    "@carforyou/eslint-config": "4.0.79",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.5",
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/addon-viewport": "7.2.1",
    "@storybook/react": "7.2.1",
    "@storybook/react-webpack5": "7.2.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "29.5.5",
    "@types/node": "16.18.59",
    "@types/react": "16.14.49",
    "@types/react-paginate": "7.1.2",
    "autoprefixer": "10.4.16",
    "bundlesize": "0.18.1",
    "css-loader": "6.8.1",
    "eslint": "8.51.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.31",
    "postcss-import": "15.1.0",
    "postcss-loader": "4.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "5.0.5",
    "rollup": "3.27.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.2",
    "semantic-release": "19.0.5",
    "storybook": "7.2.1",
    "tailwindcss": "2.2.19",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "sideEffects": false,
  "main": "./pkg/cjs/index.js",
  "module": "./pkg/esm/index.js",
  "types": "./pkg/types/src/index.d.ts",
  "files": [
    "pkg/**/*",
    "assets/**/*"
  ]
}
