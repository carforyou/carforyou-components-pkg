{
  "name": "@carforyou/components",
  "version": "0.0.0",
  "description": "CAR FOR YOU React components",
  "scripts": {
    "start": "http-server ./storybook-static -p 8008",
    "version": "npm run build",
    "dev": "npm run build:tailwind && npm run build:assets && start-storybook -s ./public -p 6006",
    "test": "jest",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix",
    "build": "npm run build:assets && npm run build:pkg",
    "build:pkg": "rimraf pkg && rollup -c && tsc --declaration --declarationDir pkg/types --emitDeclarationOnly",
    "build:tailwind": "tsc --project tsconfig.tailwind.json",
    "build:assets": "npx cfy-assets build",
    "build:storybook": "npm run build:tailwind && build-storybook",
    "bundlesize": "bundlesize",
    "deploy": "storybook-to-ghpages",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carforyou/carforyou-components-pkg.git"
  },
  "author": "CAR FOR YOU",
  "license": "MIT",
  "homepage": "https://github.com/carforyou/carforyou-components-pkg#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "tailwindcss": "^2.2.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "downshift": "^6.0.0",
    "glob": "^7.1.6",
    "lodash.debounce": "^4.0.8",
    "postcss-import": "14.0.2",
    "react-calendar": "^3.3.1",
    "react-paginate": "^7.1.5",
    "react-range": "^1.8.11",
    "react-tooltip-lite": "npm:@carforyou/react-tooltip-lite@1.12.1",
    "tailwindcss-gradients": "^2.0.0",
    "use-deep-compare-effect": "^1.4.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.0.0"
  },
  "devDependencies": {
    "@carforyou/assets": "2.1.2",
    "@carforyou/eslint-config": "4.0.0",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.4.9",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.14",
    "@types/react": "16.14.21",
    "@types/react-paginate": "7.1.1",
    "autoprefixer": "10.4.0",
    "bundlesize": "0.18.1",
    "eslint": "8.5.0",
    "http-server": "14.0.0",
    "jest": "27.3.1",
    "postcss": "8.3.11",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "4.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.2",
    "semantic-release": "17.4.7",
    "tailwindcss": "2.2.19",
    "ts-jest": "27.0.7",
    "typescript": "4.3.5"
  },
  "main": "pkg/cjs/index.js",
  "module": "pkg/esm/index.js",
  "types": "pkg/index.d.ts",
  "files": [
    "pkg/**/*"
  ]
}
